              "",
              "                        InterfaceState('EquipmentShop', 'PR_Gov', -1);",
              "                        InterfaceState('EquipmentShop', 'PR_Goods', -1);",
              "                        InterfaceState('EquipmentShop', 'PR_Shop', -1);",
              "                        InterfaceState('EquipmentShop', 'PR_Info', -1);",
