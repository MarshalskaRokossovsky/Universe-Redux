                        "                SetData(0, 1);",
                        "                SetData(0, 2);",
                        "                SetData(-3, 3);",
                        "                ChangeState('ReturningStrikeState');",
                        "                exit;",
                        "            }",
                        "            //Автоматически переводим наёмника в другую скриптовую группу, указанную в ChangeShipScriptGroupInfo",
                        "            else if(GetData(0) == 4)",
                        "            {",
                        "                ChangeShipScriptGroupFromInfo(CurShip);",
                        "                ShipFreeFlight(CurShip);",
                        "                exit;",
                        "            }",
                        "        }",
                        "    }",
                        "}",
                        "//Летим к кораблю",
                        "else if(GetData(3))",
