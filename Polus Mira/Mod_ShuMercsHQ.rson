                        "                                // if(tranc_hull_lvl >= 4)",
                        "                                // {",
                        "                                    // if(RndObject(1, 100, ship) <= tranc_hull_lvl - 1) //Бросок на шанс",
                        "                                    // {",
                        "                                        // ItemExtraSpecialsAddByType(tranc_hull, FindBonusByNameInCfg('EvoTranc_TrancAddSlotForsage'));",
