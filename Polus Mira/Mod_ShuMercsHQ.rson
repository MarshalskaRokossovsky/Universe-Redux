                        "                else if(GetData(0) == 4)",
                        "                {",
                        "                    ChangeShipScriptGroupFromInfo(CurShip);",
                        "                    ShipFreeFlight(CurShip);",
                        "                    exit;",
                        "                }",
                        "            }",
                        "        }",
                        "        else",
                        "        {",
                        "            SetData(0, 2);",
                        "            //При данном маркере после уничтожения станции наёмник автоматически направится в штаб",
                        "            if(GetData(0) == 3)",
                        "            {",
