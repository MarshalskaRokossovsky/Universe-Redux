                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "if (!quest_started && ShipIsTakeoff(Player())){",
                        "ShipOut(Player());}",
                        "if(quest_started && !quest_failed &&!system_saved) {",
                        "  if((quest_started+attack_delay+3)<CurTurn()) {",
                        "    if(!StarOwner(GalaxyStar(quest_star-1)) && !StarBattle(GalaxyStar(quest_star-1))) {",
                        "      system_saved=1;",
                        "      Ether(0,'4285D077-764A-4DA4-A6AE-3A49AB4625D1',\"Задание выполнено, можно возвращаться к заказчику на планету <Name(PlanetNew)> системы <Name(StarNew)>.\");",
                        "    }",
                        "  }",
                        "  if(StarOwner(GalaxyStar(quest_star-1))) {",
                        "    array_quest_statuses[script_num]=3;",
                        "    quest_failed=1;",
                        "    ShipOut(Player());",
                        "    Ether(5,'8EAA35ED-0990-4158-A802-EFF2751A7CB8',\"Система <Name(GalaxyStar(quest_star-1))> захвачена, ваше задание провалено. Теперь на поддержку правителя планеты <Name(PlanetNew)> можно не рассчитывать.\");",
                        "  }",
                        "}"
                    ]
                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 120,
                    "Pos.y": 465,
                    "Parent": -1,
                    "#": 25,
                    "Code.Type": "DialogBegin",
                    "Code": 
                    [
                        "if(GetShipPlanet(Player())==PlanetNew) {",
                        "  if(request_dialog && !script_replace) {",
                        "    if(DomiksDefeated()) {",
                        "      //replace",
                        "      AddDialogOverride('DialogReplace',10);",
                        "    }",
                        "    if(!DomiksDefeated()) {",
                        "      request_dialog=0;",
                        "      counter=BuildListOfQuestPossibleLocations(locations,ShipStar(Player()),1,50);",
                        "      if(counter<2) {",
                        "        AddDialogOverride('DialogPlanetCancel',10);",
                        "      }",
                        "      if(counter>1) {",
                        "        counter=ArrayDim(locations);",
                        "        while (counter>0){",
                        "        counter=counter-1;",
                        "        if (ConStar(StarToCon(locations[counter]),0)==locations[counter]){",
                        "        if (ArrayDim(locations)>1){ArrayDelete(locations,counter);}}}",
                        "        counter=ArrayDim(locations);",
                        "        if(counter<2) {",
                        "          AddDialogOverride('DialogPlanetCancel',10);",
                        "        }",
                        "        if(counter>1) {",
                        "          counter=0;",
                        "          quest_star=0;",
                        "          safety_dist=-2;",
                        "          while (counter<ArrayDim(locations)){",
                        "          if (DistToNearestEnemySystem(locations[counter])>safety_dist){",
                        "          if (StarRuins(locations[counter])>1){",
                        "          quest_star=locations[counter];",
                        "          safety_dist=DistToNearestEnemySystem(quest_star);}}",
                        "          counter=counter+1;}",
                        "          if(!quest_star) {",
                        "            AddDialogOverride('DialogPlanetCancel',10);",
                        "          }",
                        "          if(quest_star) {",
                        "            quest_star=Id(quest_star);",
                        "            AddDialogOverride('DialogPlanet',10);",
                        "          }",
                        "        }",
                        "      }",
                        "    }",
                        "  }",
                        "  if(system_saved && !quest_complete) {",
                        "    AddDialogOverride('DialogPlanetComplete',10);",
                        "  }",
                        "}"
                    ]
                },
