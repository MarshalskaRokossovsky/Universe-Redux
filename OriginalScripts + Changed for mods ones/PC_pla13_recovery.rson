                    "#": 15,
                    "Code.Type": "Init",
                    "Code": 
                    [
                        "locations=newarray(1);",
                        "counter=1;",
                        "while (counter<ArrayDim(array_script_names)){",
                        "if (array_chosen_planets[counter]==Id(PlanetNew)){script_num=counter;}",
                        "counter=counter+1;}"
                    ]
                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 60,
                    "Pos.y": 485,
                    "Parent": -1,
                    "#": 16,
                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "if (!quest_started && ShipIsTakeoff(Player())){",
                        "ShipOut(Player());}",
                        "if(GetShipPlanet(Player())) {",
                        "  if(Id(GetShipPlanet(Player()))==quest_planet) {",
                        "    if(!quest_status) {",
                        "      if((GQuestStatus==0) && !ShipInPrison(Player()) && (GetRelationPlanet(GetShipPlanet(Player()),Player())>=10)) {",
                        "        if(StartTextQuest('60')==1) {",
                        "          quest_status=1;",
                        "        }",
                        "      }",
                        "    }",
                        "  }",
                        "}",
                        "if(quest_status==1) {",
                        "  if(GQuestStatus>1) {",
                        "    quest_status=GQuestStatus;",
                        "    GQuestStatus=0;",
                        "    if(quest_status==3) {",
                        "      array_quest_statuses[script_num]=3;",
                        "      ShipOut(Player());",
                        "      Ether(5,'34EA0CCC-D6CD-4D86-848A-77E3BA236E66',\"Задание провалено. Теперь на поддержку правителя планеты <Name(PlanetNew)> можно не рассчитывать.\");",
                        "    }",
                        "    if(quest_status==2) {",
                        "      quest_item=CreateQuestItem('Usl_DataDisk');",
                        "      AddItemToShip(Player(),quest_item);",
                        "      LinkItemToScript(quest_item,Disk);",
                        "      Ether(0,'85AC7DDB-8B0B-46A5-820B-F6F16508243D',\"Диск получен, можно везти его заказчику.\");",
                        "    }",
                        "  }",
                        "}",
                        "if(quest_started) {",
                        "  if(quest_status!=4) {",
                        "    if((CurTurn()-quest_started)>max_days) {",
                        "      array_quest_statuses[script_num]=3;",
                        "      ShipOut(Player());",
                        "      quest_status=4;",
                        "      Ether(5,'34EA0CCC-D6CD-4D86-848A-77E3BA236E66',\"Время вышло, задание провалено. Теперь на поддержку правителя планеты <Name(PlanetNew)> можно не рассчитывать.\");",
                        "      if(ItemExist(Disk)) {",
                        "        ReleaseItemFromScript(Disk);",
                        "      }",
                        "    }",
                        "  }",
                        "}",
                        "if(quest_status==2) {",
                        "  if(!ItemExist(Disk)) {",
                        "    array_quest_statuses[script_num]=3;",
                        "    ShipOut(Player());",
                        "    Ether(5,'34EA0CCC-D6CD-4D86-848A-77E3BA236E66',\"Диск утерян, задание провалено. Теперь на поддержку правителя планеты <Name(PlanetNew)> можно не рассчитывать.\");",
                        "  }",
                        "}"
                    ]
                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 120,
                    "Pos.y": 485,
                    "Parent": -1,
                    "#": 17,
                    "Code.Type": "DialogBegin",
                    "Code": 
                    [
                        "if(GetShipPlanet(Player())==PlanetNew) {",
                        "  if(request_dialog) {",
                        "    request_dialog=0;",
                        "    counter=BuildListOfQuestPossibleLocations(locations,ShipStar(Player()),25,60);",
                        "    if(!counter) {",
                        "      AddDialogOverride('DialogPlanetCancel',10);",
                        "    }",
                        "    if(counter) {",
                        "      quest_planet=Id(FindPlanet(locations[0],'NotNone',0,100));",
                        "      AddDialogOverride('DialogPlanet',10);",
                        "    }",
                        "  }",
                        "  if(ItemIn(Disk,Player())) {",
                        "    AddDialogOverride('DialogComplete',10);",
                        "  }",
                        "}"
                    ]
                },
                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 0,
                    "Pos.y": 30,
                    "Parent": 21,
                    "#": 29,
                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "array_quest_statuses[script_num]=2;",
                        "khan_counter=khan_counter+1;",
                        "quest_status=4;",
                        "ShipOut(Player());",
                        "FreeItem(GetItemFromShip(Player(),FindItemInShip(Player(),Disk)));",
                        "reward_money=GalaxyMoney(4);",
                        "ShipMoney(Player(),ShipMoney(Player())+reward_money);",
                        "Ether(4,'34EA0CCC-D6CD-4D86-848A-77E3BA236E66',\"Задание выполнено, поддержка правителя планеты <Name(PlanetNew)> получена!\");"
                    ]
                },
