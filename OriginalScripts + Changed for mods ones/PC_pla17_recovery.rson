                {
                    "Type": "Top",
                    "Name": "",
                    "Pos.x": 0,
                    "Pos.y": 30,
                    "Parent": 38,
                    "#": 47,
                    "Code.Type": "Turn",
                    "Code": 
                    [
                        "quest_started=CurTurn();",
                        "array_quest_statuses[script_num]=1;",
                        "Ether(3,'8EAA35ED-0990-4158-A802-EFF2751A7CB8',\"Особое задание правителя планеты <Name(PlanetNew)> системы <Name(StarNew)><br><br>Защитить систему <Name(GalaxyStar(quest_star-1))> от нападения доминаторов, которое произойдет приблизительно <GameDateTxtByTurn(CurTurn()+attack_delay)>.\");",
                        "quest_item=CreateEquipment(t_RepairRobot,20,GalaxyTechLevel(),PlanetOwner(PlanetNew));",
                        "shmotka1=ItemFullName(quest_item);",
                        "AddItemToShip(Player(),quest_item);",
                        "quest_item=CreateEquipment(t_DefGenerator,20,GalaxyTechLevel(),PlanetOwner(PlanetNew));",
                        "shmotka2=ItemFullName(quest_item);",
                        "AddItemToShip(Player(),quest_item);",
                        "quest_item=CreateEquipment(t_Weapon9,40,GalaxyTechLevel(),PlanetOwner(PlanetNew));",
                        "shmotka3=ItemFullName(quest_item);",
                        "AddItemToShip(Player(),quest_item);",
                        "quest_item=CreateEquipment(t_Weapon10,40,GalaxyTechLevel(),PlanetOwner(PlanetNew));",
                        "shmotka4=ItemFullName(quest_item);",
                        "AddItemToShip(Player(),quest_item);",
                        "gift_money=GalaxyMoney(4);",
                        "ShipMoney(Player(),ShipMoney(Player())+gift_money);",
                        "counter=0;",
                        "while (counter<StarRuins(GalaxyStar(quest_star-1))){",
                        "RuinsAllowModernization(StarRuins(GalaxyStar(quest_star-1),counter),1);",
                        "counter=counter+1;}",
                        "quest_planet=FindPlanet(ShipStar(Player()),'NotNone',0,100);",
                        "counter=0;",
                        "while (counter<10){",
                        "quest_ship=BuyDomikExtremal(quest_planet);",
                        "MakeShipEnterStar(quest_ship,GalaxyStar(quest_star-1),StarNearbyStars(GalaxyStar(quest_star-1),counter+1),attack_delay);",
                        "counter=counter+1;}",
                        "counter=0;",
                        "while (counter<20){",
                        "quest_ship=BuyDomikExtremal(quest_planet);",
                        "MakeShipEnterStar(quest_ship,GalaxyStar(quest_star-1),StarNearbyStars(GalaxyStar(quest_star-1),counter+1),attack_delay+1);",
                        "counter=counter+1;}",
                        "counter=0;",
                        "while (counter<20){",
                        "quest_ship=BuyDomikExtremal(quest_planet);",
                        "MakeShipEnterStar(quest_ship,GalaxyStar(quest_star-1),StarNearbyStars(GalaxyStar(quest_star-1),counter+1),attack_delay+2);",
                        "counter=counter+1;}"
                    ]
                }
