                        "            if(GroupIn(QuestTarget, start_planet))",
                        "            {",
                        "                //Все уцелевшие наёмники возвращаются в штаб",
                        "                for(i = 0; i < GroupCount(QuestMercs); i = i + 1)",
                        "                {",
                        "                    tdwd1 = GroupShip(QuestMercs, i);",
                        "                    SetData(0, 0, tdwd1);",
                        "                    SetData(0, 1, tdwd1);",
