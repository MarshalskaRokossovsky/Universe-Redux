                        "        }",
                        "        else if(race == 3) {",
                        "            if(CurrentMods('ShusRangers\\ShuMM')) {",
                        "                if(GalaxyTechLevel() < 4) SpecialToEquipment(FindBonusByNameInCfg('ShuMM_FuravanHull2'),hull);",
