                        "            else if(num == 2) ArrayAdd(junk_arr, 'ShuMercsHQ_DynamiteBundle'); //Связка динамита",
                        "            else if(num == 3) ArrayAdd(junk_arr, 'ShuMercsHQ_TNT'); //Тротиловая шашка",
