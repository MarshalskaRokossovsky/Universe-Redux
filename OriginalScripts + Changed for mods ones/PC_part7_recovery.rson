                        "}",
                        "if(quest_started) {",
                        "  if(!pirate_greeting) {",
                        "    if(ShipStar(Player())==PlanetToStar(PlanetPirateClan())) {",
                        "      if(ShipInNormalSpace(Player())) {",
                        "        pirate_greeting=1;",
                        "        Ether(0,'93C088A4-DE17-49B1-BD11-77DD606D33C2',\"Добро пожаловать, <PlayerFull>! Для соблюдения всех формальностей совершите посадку на базе <Name(PlanetPirateClan())>.\");",
                        "      }",
                        "    }",
