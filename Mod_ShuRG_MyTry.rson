              "    {",
              "        if(GetShipPlanet(Player()) == PlanetPirateClan())",
              "        {",
              "            if(PlanetOwner(PlanetPirateClan()) == PirateClan) GotoGov(1);",
              "        }",
              "    }",
              "}",
              "",
              "if(GalaxyShipId() > lastShipId) {",
              "    AddDropUselessToShips();",
              "    //Улучшаем блёбиков, родившихся на Роджерии",
              "    int cnt=BuildListOfNewShips(tarr1,lastShipId,2+4+8+16,1+2+4+8+16+128,1,'');",
              "    if(cnt) {    //Если найден хотя бы один корабль",
              "        for(i=0;i<ArrayDim(tarr1);i=i+1) {",
              "            cnt=0;    //Обнуляем флаг",
              "            cur_ship=tarr1[i];    //Выбираем корабль",
              "            if(!cur_ship) continue;",
              "            if(IsPlayer(cur_ship)) continue;",
              "            if(ShipStar",
              "            tstr1=ShipType(cur_ship);",
              "            if(ShipTypeN(cur_ship) == t_Pirate || ShipTypeN(cur_ship) == t_Warrior) {",
              "            }",
              "        }",
              "    }",
              "    //Обновляем Id последнего рождённого корабля",
              "    lastShipId = GalaxyShipId();",
              "}",
              "for(i=0; i<StarShips(PlanetToStar(PlanetPirateClan())); i=i+1)",
              "{",
              "    cur_ship=StarShips(PlanetToStar(PlanetPirateClan()),i);",
              "    if(Id(cur_ship)>=ship_id && !IsPlayer(cur_ship) && GetShipPlanet(cur_ship) && ShipOnSidePirateClan(cur_ship) && ShipFindCustomShipInfoByType(cur_ship,'kostyl')==-1 && GetShipHomePlanet(cur_ship)==PlanetPirateClan())",
              "    {",
              "        ShipAddCustomShipInfo(cur_ship,'kostyl','NoShow');",
              "        j=RndObject(1,2,cur_ship);",
              "        for(j=j;j>0;j=j-1)",
              "        {",
              "            if (GetShipPirateRank(cur_ship)>=4) break;",
              "            RaisePirateRank(cur_ship);",
              "            if (GetShipPirateRank(cur_ship)==3 && !RndObject(0,2,cur_ship)) break;",
              "            if (GetShipPirateRank(cur_ship)>=4) break;",
              "        }",
              "",
              "        if (GalaxyDiffLevels(0)<=150)",
              "        {",
              "            ShipSkill(cur_ship,0,RndObject(2,6,cur_ship));",
              "            ShipSkill(cur_ship,1,RndObject(2,6,cur_ship));",
              "        }",
              "        else if (GalaxyDiffLevels(0)<=350)",
              "        {",
              "            ShipSkill(cur_ship,0,RndObject(3,6,cur_ship));",
              "            ShipSkill(cur_ship,1,RndObject(3,6,cur_ship));",
              "        }",
              "        else",
              "        {",
              "            ShipSkill(cur_ship,0,RndObject(4,6,cur_ship));",
              "            ShipSkill(cur_ship,1,RndObject(4,6,cur_ship));",
              "        }",
              "",
              "        if (GalaxyDiffLevels(0)==50) tint1 = ShipMoney(cur_ship)*1.15;",
              "        else if (GalaxyDiffLevels(0)==100) tint1 = ShipMoney(cur_ship)*1.45;",
              "        else if (GalaxyDiffLevels(0)==150) tint1 = ShipMoney(cur_ship)*1.80;",
              "        else if (GalaxyDiffLevels(0)==200) tint1 = ShipMoney(cur_ship)*2.20;",
              "        else if (GalaxyDiffLevels(0)<=300) tint1 = ShipMoney(cur_ship)*2.50;",
              "        else if (GalaxyDiffLevels(0)<=400) tint1 = ShipMoney(cur_ship)*2.80;",
              "        else if (GalaxyDiffLevels(0)<=500) tint1 = ShipMoney(cur_ship)*3.15;",
              "        ShipMoney(cur_ship,tint1);",
              "        ShipRefit(cur_ship);",
              "",
              "        if (GalaxyDiffLevels(0)==50) tint1 = RndObject(1,2,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==100) tint1 = RndObject(1,4,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==150) tint1 = RndObject(2,6,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==200) tint1 = RndObject(3,8,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==250) tint1 = RndObject(3,9,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==300) tint1 = RndObject(3,10,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==350) tint1 = RndObject(4,10,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==400) tint1 = RndObject(5,10,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==450) tint1 = RndObject(5,11,cur_ship);",
              "        else if (GalaxyDiffLevels(0)==500) tint1 = RndObject(6,11,cur_ship);",
              "        ShipImproveItems(cur_ship,tint1);",
              "    }",
              "}",
              "ship_id=GalaxyShipId();",
