                        "        if(CurrentForm() == 'Scaner')",
                        "        {",
                        "            DeleteRewardByNom(FormCurShip(), -1, 100000);",
                        "            RemoveIllsAndStims(FormCurShip());",
                        "        }",
                        "    }",
                        "}",
