          [
              "//Задачи для улучшения данной модификации",
              "//",
              "//Добавить период \"льготных процентов\" - повторяющиеся события снижения ключевой ставки, что даёт сниженный процент",
              "//Кредитная история",
              "//Рефинансирование кредитов",
              "//Несколько кредитов",
              "//Задания от БЦ для повышения процента вклада или промотки условно 100-200 ходов (т.е. дополнительный прибыток от депозита)",
              "//Крутые торгаши производят финансирования",
              "//Финансирование строительства линкоров",
              "//Новости-рассылка от банка при взятии кредитки",
              "//Не забудь поставить в описание номер кредитки, а то что, зря ты его, что ли, рандомил?",
              "",
              "//Начисление процентов по кредитке",
              "if(PercentCD == 0 && CardFlag == 1) {",
              "    temp_int_4 = UselessItemData(IdToItem(CardID),1); //текущая недостача",
              "    temp_int_5 = UselessItemData(IdToItem(CardID),2); //суммарный баланс",
              "    if(temp_int_4 != 0) {",
              "        UselessItemData(IdToItem(CardID),1,round(UselessItemData(IdToItem(CardID),1) * 108 / 100));",
              "        Sound('Sound.NewMsg');",
              "        NewsAdd(\"Начислены проценты по кредитной карте\");",
              "    }",
              "    PercentCD = 31;",
              "}",
              "if(PercentCD != 0 && CardFlag == 1) PercentCD = PercentCD - 1;",
              "",
              "//Льготная ставка",
              "if(Lgot == 0) {",
              "    Lgot = 1070;",
              "}",
              "if(Lgot == 1000) {",
              "    Lgot = Rnd(450,980);",
              "}",
              "",
              "//Выплата при смерти чего-нибудь",
              "if(!ShipIsTakeoff(Player())) {",
              "    for(i=GalaxyEvents()-1; i>=0; i=i-1) {",
              "        if(GalaxyEventDate(i) < CurTurn()) break;",
              "        if(GalaxyEventType(i)=='PlayerNodesNullified') { //Нод-счёт игрока обнулякался",
              "            if(GalaxyEventData(i,0) != 0 && PlayerMedPolicy() > 0) {",
              "                ShipMoney(Player(),ShipMoney(Player())+(GalaxyEventData(i,0)*25));",
              "                Sound('Sound.Buy');",
              "                NewsAdd(\"В связи с уничтожением всех центров рейнджеров Сеть Бизнес-Центров выплачивает вам компенсацию в размере \"+(GalaxyEventData(i,0)*25)+\" cr.\")",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "//Блок перелистывания кулдаунов",
              "if(Lgot != 0) Lgot = Lgot - 1;",
              "if(BancCD != 0) BancCD = BancCD - 1;",
              "if(LinerCD != 0) LinerCD = LinerCD - 1;",
              "if(ColonCD != 0) ColonCD = ColonCD - 1;",
              "if(ScienceCD != 0) ScienceCD = ScienceCD - 1;",
              "if(MilitiaCD != 0) MilitiaCD = MilitiaCD - 1;",
              "if(LinersCD != 0) LinersCD = LinersCD - 1;"          
          ]        
        },
