          [
              "if (ShipInStar(Player(), Star)) {",
              "  if (vState == 1) {",
              "    if (!ItemIn(iCapsuleP, Player()) && iCapsuleP) {",
              "        if(Dist(iCapsuleP, Player()) <= 1000) {",
              "            Ether(1, '8E4B1F72-8A0A-441E-9C5F-49AF80B98E36', \"А-а-а-а-а-а!!! Я два дня просидел в этой проклятой штуковине, и она, похоже, не открывается ни изнутри, ни снаружи! Отвези меня... нет, военные меня посадят или даже расстреляют за то, что я сделал... На пиратскую базу меня отвези. За это получишь от меня... Черт, ничего ведь нет... Э-ээ... А, получишь пушку крутую. Черт, черт, черт! В этой жестянке обратной связи нет... Короче, рейнджер, надеюсь на твою жадность - помоги мне, а я помогу тебе... Я в капсуле спасательной. На месте, где <clr>Научная База<clrEnd> <vNewSBName>... была...\");",
              "        }",
              "    }",
              "    else vState = 2;",
              "  }",
              "}",
              "if (vState == 1 || vState == 2) {",
              "    if(!ItemExist(iCapsuleP)) AllShipOut();",
              "}",
              "if(vState == 3 && CurTurn() >= vTimer) {",
              "    if(CountIn(PBGroup)) {",
              "        if(ShipStar(Player()) == ShipStar(GroupToShip(PBGroup))) {",
              "            if(Dist(Player(),GroupToShip(PBGroup))) {",
              "                NewsAdd(\"<Player>, это опять я. Собственно, презент готов: я восстановил по памяти ручной фазер, теперь он твой. Я его выброшу в шлюз сейчас, а ты подлети и подбери его около базы. До встречи!\");",
              "                Sound('Sound.NewMsg');",
              "                vState = 4;",
              "            }",
              "            else {",
              "                NewsAdd(\"Эй, <Player>! Подлети поближе к пиратской базе. Тут с тобой хотят поговорить\"+Format('FormInfo.br')+\"Пиратская база\"+' '+Name(GroupToShip(PBGroup)));",
              "                Sound('Sound.NewMsg');",
              "            }",
              "        }",
              "    }",
              "}"          
