              "                    if(!(is_equip_purchase % 2)) cur_store = IdToPlanet(is_equip_purchase / 2); //Магазин на планете",
              "                    else cur_store = IdToShip((is_equip_purchase - 1) / 2); //Магазин на станции",
              "",
              "                    tarr = newarray(1);",
