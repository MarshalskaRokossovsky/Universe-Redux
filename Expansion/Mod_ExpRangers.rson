                        "                ItemImprovement(item,2,2);",
                        "                AddItemToShip(ship,item);",
                        "                ItemIsInUse(item,ship,1);",
                        "                NoDropItem(item,1);",
                        "",
                        "                if(CurrentMods('ShusRangers\\ShuMM')) {",
                        "                    SpecialToEquipment(item,FindBonusByNameInCfg('ShuMM_ExtremumSplinter'));",
