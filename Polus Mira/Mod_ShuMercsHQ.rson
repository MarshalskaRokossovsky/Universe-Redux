                        "                                    ShipJoin(MercsAttackers,cur_ship);",
                        "                                    MakeShipEnterStar(cur_ship,ShipStar(cur_ruin),StarNearbyStars(ShipStar(cur_ruin),Rnd(1,20)),2);",
                        "                                }",
                        "                            }",
                        "                        }",
                        "                        AttackCD = 0;",
                        "                    }",
