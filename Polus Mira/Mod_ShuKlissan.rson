                        "    {",
                        "        //Если в этой системе таки условно есть нейролинк, просто временно находится на перегруппировке",
                        "        if(int(GetVariableName(KlissanInvasion['RegroupingNeurolinks', i])) == star)",
                        "        {",
                        "            result = 1;",
                        "            exit;",
                        "        }",
                        "    }",
                        "",
                        "    result = 0;",
                        "}",
