                    "Parent": -1,
                    "#": 7,
                    "Code.Type": "DialogBegin",
                    "Code": 
                    [
                        "if(GetShipRuins(Player())) {",
                        "    if(!PlayerBridge()) {",
                        "        tdwd = GetShipRuins(Player());",
                        "        if(ShipType(tdwd) == 'TC') {",
                        "            if(GalaxyTechLevel() >= 5) {",
                        "                if(!playerGotECM) {",
                        "                    AddDialogInject('BuyECM', \"Недавно мы наладили серийное производство точечных усилителей поля, не желаете взглянуть?\", \"Приобрести точечный усилитель поля\", 1);",
                        "                }",
                        "            }",
                        "        }",
                        "        else if (ShipType(tdwd) == 'WB') {",
                        "          if (!ShipOnSidePirateClan(Player())) {",
                        "            if(!playerGotAbsorber) {",
                        "              if (ShipRank(Player()) >= 2) {",
                        "                AddDialogInject('BuyAbsorber', \"Не желаете ли также приобрести абсорбер?\", \"Хочу приобрести абсорбер\", 1);",
                        "              }",
                        "            }",
                        "          }",
                        "        }",
                        "        else if(ShipType(tdwd) == 'CB') {",
                        "          if (ShipOnSidePirateClan(Player())) {",
                        "            if(!playerGotAbsorber) {",
                        "              if (GetShipPirateRank(Player()) >= 2) {",
                        "                AddDialogInject('BuyAbsorber', \"Не желаете ли также приобрести абсорбер?\", \"Хочу приобрести абсорбер\", 1);",
                        "              }",
                        "            }",
                        "          }",
                        "        }",
                        "    }",
                        "}"
                    ]
                },
